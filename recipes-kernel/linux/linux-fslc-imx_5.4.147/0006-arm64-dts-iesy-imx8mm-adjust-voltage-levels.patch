From bcc8888a83e766115c615e8a4a053e233a7b769d Mon Sep 17 00:00:00 2001
From: Dominik Poggel <pog@iesy.com>
Date: Thu, 5 Jan 2023 12:19:46 +0100
Subject: [PATCH 6/6] arm64: dts: iesy: imx8mm: adjust voltage levels

---
 .../boot/dts/iesy/iesy-imx8mm-eva-mi.dts      | 32 +++++++++----------
 1 file changed, 16 insertions(+), 16 deletions(-)

diff --git a/arch/arm64/boot/dts/iesy/iesy-imx8mm-eva-mi.dts b/arch/arm64/boot/dts/iesy/iesy-imx8mm-eva-mi.dts
index 523c35a20c75..7a82ede74f4c 100755
--- a/arch/arm64/boot/dts/iesy/iesy-imx8mm-eva-mi.dts
+++ b/arch/arm64/boot/dts/iesy/iesy-imx8mm-eva-mi.dts
@@ -57,10 +57,10 @@
 
 	reg_sd1_vmmc: sd1_regulator {
 		compatible = "regulator-fixed";
-		regulator-name = "WLAN_EN";
-		regulator-min-microvolt = <3300000>;
-		regulator-max-microvolt = <3300000>;
-		gpio = <&gpio2 10 GPIO_ACTIVE_HIGH>;
+		regulator-name = "1V8_IO_reg";
+		regulator-min-microvolt = <1800000>;
+		regulator-max-microvolt = <1800000>;
+		gpio = <&gpio2 10 GPIO_ACTIVE_HIGH>; /* NC */
 		off-on-delay-us = <20000>;
 		startup-delay-us = <100>;
 		enable-active-high;
@@ -70,9 +70,9 @@
 		compatible = "regulator-fixed";
 		pinctrl-names = "default";
 		pinctrl-0 = <&pinctrl_reg_usdhc2_vmmc>;
-		regulator-name = "VSD_3V3";
-		regulator-min-microvolt = <3300000>;
-		regulator-max-microvolt = <3300000>;
+		regulator-name = "VCC_SD_reg";
+		regulator-min-microvolt = <1800000>;
+		regulator-max-microvolt = <1800000>;
 		gpio = <&gpio2 19 GPIO_ACTIVE_HIGH>;
 		off-on-delay-us = <20000>;
 		enable-active-high;
@@ -301,20 +301,20 @@
 		regulators {
 			vdd_soc_0v8: BUCK1 {
 				regulator-name = "0v8_soc";
-				regulator-min-microvolt = <850000>;
-				regulator-max-microvolt = <850000>;
+				regulator-min-microvolt = <700000>;
+				regulator-max-microvolt = <1300000>;
 			};
 
 			vdd_arm_0v9: BUCK2 {
 				regulator-name = "0v9_arm";
-				regulator-min-microvolt = <850000>;
-				regulator-max-microvolt = <850000>;
+				regulator-min-microvolt = <700000>;
+				regulator-max-microvolt = <1300000>;
 			};
 
 			vdd_dram_0v9: BUCK3 {
 				regulator-name = "0v9_dram";
-				regulator-min-microvolt = <850000>;
-				regulator-max-microvolt = <850000>;
+				regulator-min-microvolt = <700000>;
+				regulator-max-microvolt = <1300000>;
 			};
 
 			nvcc_3v3: BUCK4 {
@@ -344,7 +344,7 @@
 			vdd_snvs_0v8: LDO2 {
 				regulator-name = "0v8_snvs";
 				regulator-min-microvolt = <800000>;
-				regulator-max-microvolt = <800000>;
+				regulator-max-microvolt = <900000>;
 			};
 
 			vdda_1v8: LDO3 {
@@ -361,8 +361,8 @@
 
 			vcc_sd: LDO5 {
 				regulator-name = "vcc_sd";
-				regulator-min-microvolt = <3300000>;
-				regulaotr-max-microvolt = <3300000>;
+				regulator-min-microvolt = <1800000>;
+				regulaotr-max-microvolt = <1800000>;
 			};
 		};
 	};
